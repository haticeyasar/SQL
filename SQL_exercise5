--film tablosunda bulunan ve film ismi (title) 'n' karakteri ile biten en uzun (length) 5 film
  SELECT * FROM film WHERE title LIKE '%n' ORDER BY film.length DESC LIMIT 5
  
--film tablosunda bulunan ve film ismi (title) 'n' karakteri ile biten en kısa (length) ikinci 5 film(6,7,8,9,10)
  SELECT * FROM film WHERE title LIKE '%n' ORDER BY film.length ASC LIMIT 5
  
--customer tablosunda bulunan last_name sütununa göre azalan yapılan sıralamada store_id 1 olmak koşuluyla ilk 4 veri
  SELECT * FROM customer WHERE store_id=1 ORDER BY last_name DESC LIMIT 4
